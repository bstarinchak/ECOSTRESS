	keywords = {below-ground, biomass, carbon reserves, defoliation, fine roots, hurricane, radiocarbon, rhizosphere},
	number = {3},
	pages = {710-718},
	title = {Evidence of old carbon used to grow new fine roots in a tropical forest},
