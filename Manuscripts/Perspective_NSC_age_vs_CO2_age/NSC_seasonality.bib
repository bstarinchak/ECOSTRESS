	volume = {21},
	year = {2007},
	Bdsk-Url-1 = {https://doi.org/10.1007/s00468-006-0118-7}}

@article{krepkowski:2013,
	abstract = {Summary We present an intra-annual stable carbon isotope (δ13C) study based on a labeling experiment to illustrate differences in temporal patterns of recent carbon allocation to wood structures of two functional types of trees, Podocarpus falcatus (a late-successional evergreen conifer) and Croton macrostachyus (a deciduous broadleaved pioneer tree), in a tropical mountain forest in Ethiopia. Dendrometer data, wood anatomical thin sections, and intra-annual δ13C analyses were applied. Isotope data revealed a clear annual growth pattern in both studied species. For P. falcatus, it was possible to synchronize annual δ13C peaks, wood anatomical structures and monthly precipitation patterns. The labeling signature was evident for three consecutive years. For C. macrostachyus, isotope data illustrate a rapid decline of the labeling signal within half a year. Our δ13C labeling study indicates a distinct difference in carryover effects between trees of different functional types. A proportion of the labeled δ13C is stored in reserves of wood parenchyma for up to 3 yr in P. falcatus. By contrast, C. macrostachyus shows a high turnover of assimilates and a carbon carryover effect is only detectable in the subsequent year.},
	author = {Krepkowski, Julia and Gebrekirstos, Aster and Shibistova, Olga and Br{\"a}uning, Achim},
	date-added = {2021-04-08 15:57:23 +0200},
	date-modified = {2021-04-08 15:57:34 +0200},
	doi = {https://doi.org/10.1111/nph.12266},
