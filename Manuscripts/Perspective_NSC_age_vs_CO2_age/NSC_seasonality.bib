	eprint = {https://nph.onlinelibrary.wiley.com/doi/pdf/10.1111/nph.12064},
	journal = {New Phytologist},
	keywords = {carbon limitation, drought mortality, hydraulic conductance, hydraulic failure, nonstructural carbohydrates, plant strategy},
	number = {3},
	pages = {862-872},
	title = {Drought response strategies define the relative contributions of hydraulic dysfunction and carbohydrate depletion during tree mortality},
	url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.12064},
	volume = {197},
	year = {2013},
	Bdsk-Url-1 = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.12064},
