	journal = {International journal of molecular sciences},
	keywords = {NSC; PLC; drought; embolism; hydraulic recovery; pH; starch; stem; sugars; xylem sap; Carbohydrates/physiology; Droughts; Parenchymal Tissue/metabolism; Starch/*metabolism; Trees/metabolism/physiology; Xylem/*metabolism},
