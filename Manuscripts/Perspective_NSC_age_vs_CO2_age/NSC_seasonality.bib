	journal = {Global Biogeochemical Cycles},
	keywords = {GPP, PCE, autotrophic respiration, NPP, tropical forests},
	number = {5},
	pages = {645-655},
	title = {Source and sink carbon dynamics and carbon allocation in the Amazon basin},
