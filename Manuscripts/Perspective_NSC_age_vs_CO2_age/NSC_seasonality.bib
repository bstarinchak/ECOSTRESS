	doi = {https://doi.org/10.1111/nph.16461},
	eprint = {https://nph.onlinelibrary.wiley.com/doi/pdf/10.1111/nph.16461},
	journal = {New Phytologist},
	keywords = {carbon ages and transit times, carbon allocation, modeling, nonstructural carbohydrates, tree carbon dynamics, tree storage dynamics},
	number = {5},
	pages = {1299-1311},
	title = {Probability distributions of nonstructural carbon ages and transit times provide insights into carbon allocation dynamics of mature trees},
	url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.16461},
	volume = {226},
