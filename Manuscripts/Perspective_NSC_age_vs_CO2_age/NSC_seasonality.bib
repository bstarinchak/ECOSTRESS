	doi = {https://doi.org/10.1111/pce.13858},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/pce.13858},
	journal = {Plant, Cell \& Environment},
	keywords = {climate ghange, Fagus sylvatica L. (beech), growth, phenology, resilience, wood formation},
	number = {10},
	pages = {2365-2379},
	title = {Frost and drought: Effects of extreme weather events on stem carbon dynamics in a Mediterranean beech forest},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/pce.13858},
	volume = {43},
	year = {2020},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/pce.13858},
