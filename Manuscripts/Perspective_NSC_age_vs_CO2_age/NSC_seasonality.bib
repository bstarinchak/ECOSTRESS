	journal = {Plant, Cell \& Environment},
	keywords = {bark photosynthesis, bark thickness, non-structural carbohydrate pools, root storage, secondary phloem, soluble sugars, starch, wood density, wood storage},
	number = {1},
	pages = {156-170},
	title = {Inner bark as a crucial tissue for non-structural carbohydrate storage across three tropical woody plant communities},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/pce.13903},
	volume = {44},
	year = {2021},
