	journal = {Geophysical Research Letters},
	keywords = {carbon budget, plant regrowth, CO2 fertilization, land use change, biosphere model},
	number = {10},
	pages = {4820-4830},
	title = {Plant Regrowth as a Driver of Recent Enhancement of Terrestrial CO2 Uptake},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2018GL077633},
