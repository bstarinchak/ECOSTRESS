	id = {H{\"o}ll1975},
	isbn = {978-3-642-66161-7},
	pages = {432-450},
	publisher = {Springer Berlin Heidelberg},
	title = {Radial Transport in Rays},
	ty = {CHAP},
	url = {https://doi.org/10.1007/978-3-642-66161-7_18},
