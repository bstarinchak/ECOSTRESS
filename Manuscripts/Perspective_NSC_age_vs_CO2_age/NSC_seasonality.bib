	isbn = {1664-462X; 1664-462X},
	j2 = {Front Plant Sci},
	journal = {Frontiers in plant science},
	keywords = {cutting plane; measured wood surface; measurement accuracy; non-structural carbohydrates (NSC); number of samples; ray density; ray dimensions; ray volume},
	l2 = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4649045/},
	la = {eng},
	month = {11},
	pages = {1016--1016},
	publisher = {Frontiers Media S.A.},
	title = {Assessing Conifer Ray Parenchyma for Ecological Studies: Pitfalls and Guidelines},
	ty = {JOUR},
	u1 = {26635842{$[$}pmid{$]$}},
