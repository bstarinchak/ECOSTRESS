	keywords = {carbon allocation, free-air CO2 enrichment (FACE), fungi, rhizosphere, roots, soil, soil respiration, stable isotopes},
	number = {2},
	pages = {319-329},
	title = {Canopy CO2 enrichment permits tracing the fate of recently assimilated carbon in a mature deciduous forest},
	url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/j.1469-8137.2006.01831.x},
