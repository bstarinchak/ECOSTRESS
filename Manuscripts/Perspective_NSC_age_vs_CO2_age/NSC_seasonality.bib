	journal = {New Phytologist},
	keywords = {δ13C, carbon assimilation, gross primary productivity (GPP), leaf gas-exchange, vapor pressure deficit, water-use efficiency (WUE)},
	number = {1},
	pages = {436-446},
	title = {Association between sap flow-derived and eddy covariance-derived measurements of forest canopy CO2 uptake},
	url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.13597},
	volume = {209},
	year = {2016},
	Bdsk-Url-1 = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.13597},
