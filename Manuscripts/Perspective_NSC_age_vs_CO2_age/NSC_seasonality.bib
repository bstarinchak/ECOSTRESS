	journal = {New Phytologist},
	keywords = {carryover effect, dendrometer, Ethiopia, growth dynamics, stable carbon isotopes, tropical dendrochronology, wood anatomy},
	number = {2},
	pages = {431-440},
	title = {Stable carbon isotope labeling reveals different carry-over effects between functional types of tropical trees in an Ethiopian mountain forest},
	url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.12266},
	volume = {199},
